  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "node src/migrations/run.js",
    "migrate:status": "node src/migrations/status.js",
    "migrate:rollback": "node src/migrations/rollback.js",
    "seed": "node src/seeds/run.js",
